{
  "name": "amqp-telegram-gateway",
  "version": "1.0.0",
  "description": "Microservice that routes all the telegram queries and manages permissions.",
  "main": "src/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "npm run build && npm run start",
    "build": "tsc",
    "prettier-format": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "amqp",
    "rabbitmq",
    "telegram",
    "bot",
    "gateway"
  ],
  "author": "tsziming",
  "license": "MIT",
  "dependencies": {
    "amqplib": "^0.8.0",
    "dotenv": "^16.0.0",
    "grammy": "^1.7.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3"
  },
  "repository": "github.com/tsziming/amqp-telegram-gateway",
  "packageManager": "yarn@3.2.0"
}
